// babel-jest 사용하여 ES 모듈 지원 추가 (선택사항)
{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "env": {
    "test": {
      "plugins": ["babel-plugin-transform-import-meta"]
    }
  }
}
